colors = ["red", "blue", "green", "pink", "blue"]

unique_colors = set()
for color in colors:
    unique_colors.add(color)
print(unique_colors)


words = ["This", "is", "cool", "mangoes", "oranges", "apple"]

uniqu = {len(word) for word in words}
print(uniqu)

squares = {x: x**2 for x in range(1, 11)}
print(squares)
